<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude cache files from cloud backup -->
        <exclude domain="cache" />
        
        <!-- Exclude temporary files from cloud backup -->
        <exclude domain="file" path="temp/" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="sharedpref" path="device_prefs.xml" />
    </device-transfer>
</data-extraction-rules>