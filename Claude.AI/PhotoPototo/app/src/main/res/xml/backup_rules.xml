<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Exclude cache files -->
    <exclude domain="cache" />
    
    <!-- Exclude temporary files -->
    <exclude domain="file" path="temp/" />
    
    <!-- Exclude sensitive data -->
    <exclude domain="sharedpref" path="device_prefs.xml" />
</full-backup-content>